<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="logImpl" value="LOG4J" />
		<setting name="cacheEnabled" value="false"/>
	</settings>
	<typeAliases>
		<typeAlias type="gambler.examples.scheduler.domain.auth.Permission" alias="Permission" />
		<typeAlias type="gambler.examples.scheduler.domain.auth.User" alias="User" />
		<typeAlias type="gambler.examples.scheduler.domain.auth.Role" alias="Role" />
		<typeAlias type="gambler.examples.scheduler.domain.auth.UserRole" alias="UserRole" />
		<typeAlias type="gambler.examples.scheduler.domain.auth.UserPermission" alias="UserPermission" />
		<typeAlias type="gambler.examples.scheduler.domain.auth.RolePermission" alias="RolePermission" />
	</typeAliases>
	<mappers>
		<mapper resource="sqlmap/AuthDaoMapper.xml"/>
	</mappers>
</configuration>