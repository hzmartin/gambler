<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="gambler.commons.persistence.test.SysAppLevel" table="SysAppLevel">

		<id name="id" column="id" type="long">
		</id>

		<property name="productKey" type="string" column="productKey"
			length="80" not-null="false" />

		<property name="platform" type="string" column="platform"
			length="80" not-null="false" />

		<property name="level" type="string" column="level" length="80"
			not-null="false" />

		<property name="displayName" type="string" column="displayName"
			length="80" not-null="false" />

		<property name="parent" type="long" column="parent" not-null="false" />

		<property name="enable" type="integer" column="enable"
			not-null="false" />
	</class>
</hibernate-mapping>
