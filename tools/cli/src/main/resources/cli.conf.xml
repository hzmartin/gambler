<map>
	<prop namespace="password">
		<key name="algorithm">PBEWithMD5AndDES</key>
		<key name="envName">GAMBLER_PASSWD</key>
	</prop>
	<prop namespace="cli">
		<key name="maxHistorySize">1000</key>
		<key name="debug">on</key>
	</prop>
    <prop namespace="cmd">
        <!-- make sure the unique name for each command -->
        <key name="1">gambler.tools.cli.cmd.PasswordCommand</key>
        <key name="2">gambler.tools.cli.cmd.GannCommand</key>
    </prop>
</map>